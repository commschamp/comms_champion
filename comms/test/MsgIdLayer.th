    std::size_t missingSize2 = 0U;
    std::size_t msgIndex = 0U;
    MessageType msgId = MessageType();
    auto es =
        stack.read(
            msg,
            readIter,
            BufSize,
            comms::protocol::missingSize(missingSize),
            comms::protocol::msgId(msgId),
